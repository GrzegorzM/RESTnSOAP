@model PartTimeEmployee

@using ClientWebApplication.EmployeeService

<div id="part-job-container" style="display: none;">
    <div class="form-group">
        @Html.LabelFor(x => x.HourlyPay)
        @Html.EditorFor(x => x.HourlyPay, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(x => x.HoursWorked)
        @Html.EditorFor(x => x.HoursWorked, new { htmlAttributes = new { @class = "form-control" } })
    </div>
</div>